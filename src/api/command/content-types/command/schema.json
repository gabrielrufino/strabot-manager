{
  "kind": "collectionType",
  "collectionName": "commands",
  "info": {
    "singularName": "command",
    "pluralName": "commands",
    "displayName": "Command",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Command": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^\\S+$"
    },
    "Active": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "messages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::message.message",
      "inversedBy": "commands"
    },
    "Description": {
      "type": "text"
    }
  }
}
